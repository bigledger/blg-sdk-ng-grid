{
  "name": "avatar-3d",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/avatar-3d/src",
  "prefix": "ng-ui",
  "projectType": "library",
  "tags": ["avatar", "3d", "three.js"],
  "targets": {
    "build": {
      "executor": "@nx/angular:ng-packagr-lite",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "libs/avatar-3d/ng-package.json",
        "tsConfig": "libs/avatar-3d/tsconfig.lib.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/avatar-3d/tsconfig.lib.prod.json"
        },
        "development": {}
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/avatar-3d/jest.config.ts",
        "tsConfig": "libs/avatar-3d/tsconfig.spec.json"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}